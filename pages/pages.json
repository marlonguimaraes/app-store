{
  "extensions": {
    "store/theme": {
      "component": "Theme"
    },
    "store/__loading": {
      "component": "vtex.styleguide/Spinner"
    }
  },
  "routes": {
    "store/review": {
      "path": "/:slug/r"
    },
    "store/checkout": {
      "path": "/checkout"
    },
    "store/order-placed": {
      "path": "/order-placed"
    }
  },
  "templates": {
    "store": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/StoreContextProvider",
      "props": {
        "elements": [
          "header",
          "__children__",
          "footer"
        ]
      },
      "extensions": {
        "header": {
          "component": "Header"
        },
        "footer": {
          "component": "Footer"
        }
      }
    },
    "home": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/StoreContextProvider",
      "props": {
        "elements": [
          "jumbotron",
          "published",
          "soon"
        ]
      },
      "extensions": {
        "jumbotron": {
          "component": "Jumbotron"
        },
        "published": {
          "component": "AppShelf",
          "props": {
            "specificationFilters": "Published",
            "to": 12,
            "title": "apps"
          }
        },
        "soon": {
          "component": "AppShelf",
          "props": {
            "specificationFilters": "Coming Soon",
            "to": 12,
            "title": "comingSoon"
          }
        }
      }
    },
    "product": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductContextProvider",
      "props": {
        "elements": [
          "details",
          "gallery"
        ]
      },
      "extensions": {
        "details": {
          "component": "ProductContainer"
        },
        "gallery": {
          "component": "AppGallery",
          "props": {
            "productPage": true
          }
        }
      }
    },
    "checkout": {
      "component": "Checkout"
    },
    "order-placed": {
      "component": "OrderPlaced"
    },
    "review": {
      "component": "ReviewOrderPage"
    }
  },
  "pages": {
    "store": [
      {
        "name": "Default",
        "template": "store"
      }
    ],
    "store/home": [
      {
        "name": "Default",
        "template": "home"
      }
    ],
    "store/product": [
      {
        "name": "Default",
        "template": "product"
      }
    ],
    "store/checkout": [
      {
        "name": "Default",
        "template": "checkout"
      }
    ],
    "store/order-placed": [
      {
        "name": "Default",
        "template": "order-placed"
      }
    ],
    "store/review": [
      {
        "name": "Default",
        "template": "review"
      }
    ]
  }
}