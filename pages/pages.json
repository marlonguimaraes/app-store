{
  "pages": {
    "store": {
      "path": "/",
      "theme": "theme"
    },
    "store/product": {
      "path": "/:slug/p"
    },
    "store/review": {
      "path": "/:slug/r"
    }
  },
  "extensions": {
    "store": {
      "component": "StoreTemplate"
    },
    "store/product": {
      "component": "ProductPage"
    },
    "store/review": {
      "component": "ReviewOrderPage"
    },
    "store/home/shelf": {
      "component": "vtex.shelf/Shelf"
    },
    "store/home/appItem": {
      "component": "AppItem"
    }
  }
}
